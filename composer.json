{
  "name": "xprojects/contao-mobilemenu",
  "description": "Mobilemenu for Contao",
  "keywords": [
    "contao",
    "mobilemenu"
  ],
  "type": "contao-bundle",
  "version": "1.0.0",
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": ">=7.4",
    "contao/core-bundle": "^4.9"
  },
  "authors": [
    {
      "name": "XProjects",
      "homepage": "https://x-projects.de",
      "email": "infos@x-projects.de",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "XProjects\\Mobilemenu\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "XProjects\\Mobilemenu\\ContaoManager\\Plugin",
    "contao-component-dir": "public"
  },
  "config": {
    "allow-plugins": true
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "contao/manager-bundle": "^4.13",
    "contao/manager-plugin": "^2.0",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0",
    "phpstan/phpstan-symfony": "^1.0",
    "slam/phpstan-extensions": "^6.0",
    "phpunit/phpunit": "^9.5",
    "symfony/phpunit-bridge": "^5.2"
  }
}
